{
  "name": "Template",
  "description": "Template",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "/icons/favicon16.png",
      "32": "/icons/favicon32.png",
      "48": "/icons/favicon48.png",
      "128": "/icons/favicon128.png"
    }
  },
  "icons": {
    "16": "/icons/favicon16.png",
    "32": "/icons/favicon32.png",
    "48": "/icons/favicon48.png",
    "128": "/icons/favicon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "webRequest",
    "webNavigation",
    "background"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://leetcode.com/problemset/*"
      ],
      "js": [
        "./manipulate.js"
      ],
      "world": "MAIN"
    }
  ]
}
