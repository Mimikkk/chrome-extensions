{
  "name": "Leetcode Problemset.",
  "description": "Remove from the view all premium or daily problems.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/icons/favicon16.png",
    "32": "/icons/favicon32.png",
    "48": "/icons/favicon48.png",
    "128": "/icons/favicon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "webRequest",
    "webNavigation",
    "background"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://leetcode.com/problemset/*"
      ],
      "js": [
        "./manipulate.js"
      ],
      "world": "MAIN"
    }
  ]
}
