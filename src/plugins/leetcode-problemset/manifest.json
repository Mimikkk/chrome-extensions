{
  "name": "Template",
  "description": "Template",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "/icons/favicon16.png",
      "32": "/icons/favicon32.png",
      "48": "/icons/favicon48.png",
      "128": "/icons/favicon128.png"
    }
  },
  "icons": {
    "16": "/icons/favicon16.png",
    "32": "/icons/favicon32.png",
    "48": "/icons/favicon48.png",
    "128": "/icons/favicon128.png"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "*://[*.]leetcode.com/problemset/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://[*.]leetcode.com/problemset/*"
      ],
      "js": [
        "./index.js"
      ],
      "world": "MAIN"
    }
  ]
}
